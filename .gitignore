# Celery stuff
celerybeat-schedule
celerybeat.pid

# SageMath parsed files
*.sage.py

# Environments
.env
.venv
env/
venv/
ENV/
env.bak/
venv.bak/

# Spyder project settings
.spyderproject
.spyproject

# Rope project settings
.ropeproject

# mkdocs documentation
/site

# mypy
.mypy_cache/
.dmypy.json
dmypy.json

# Pyre type checker
.pyre/

# pytype static type analyzer
.pytype/

# Cython debug symbols
cython_debug/

# ============================================================================
# DATA FILES (Large genomic data - DO NOT COMMIT)
# ============================================================================

# Raw input data (PDB files can be 100MB+)
data/raw/*.pdb
data/raw/*.cif
data/raw/*.ent
data/raw/*.gz
data/raw/*.zip
data/raw/*.tar
data/raw/*.tar.gz

# Processed intermediate data
data/processed/
data/processed/**/*

# Keep directory structure
!data/raw/.gitkeep
!data/processed/.gitkeep

# ============================================================================
# RESULTS / OUTPUT FILES (User-generated - DO NOT COMMIT)
# ============================================================================

# All results directories
results/
results/**/*

# Specific output file types
*.html
*.png
*.jpg
*.jpeg
*.pdf
*.svg

# But ALLOW small examples and documentation
!examples/sample_outputs/*.html
!examples/sample_outputs/*.png
!docs/images/*.png
!docs/images/*.jpg
!docs/**/*.png
!docs/**/*.jpg
!README_images/*.png

# Keep directory structure
!results/.gitkeep

# ============================================================================
# NEXTFLOW
# ============================================================================

# Nextflow work directory (can be GBs)
work/
work/**/*

# Nextflow cache and logs
.nextflow/
.nextflow.log
.nextflow.log.*

# Nextflow history
.nextflow/history

# Nextflow timeline/trace/report (auto-generated)
timeline.html
trace.txt
report.html
dag.html
*.html.* 

# Nextflow temp files
.command.*
.exitcode

# But ALLOW example reports in docs
!docs/examples/*.html

# ============================================================================
# AWS / CLOUD
# ============================================================================

# AWS credentials (CRITICAL - never commit)
.aws/
*.pem
*.key
credentials
config

# AWS specific
.aws-sam/

# Terraform state files (if using IaC)
*.tfstate
*.tfstate.*
.terraform/
.terraform.lock.hcl

